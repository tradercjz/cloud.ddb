# Database
DATABASE_URL=postgresql+asyncpg://username:password@localhost/dolphindb_cloud

# Security
SECRET_KEY=your-secret-key-here

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Alibaba Cloud
ALIYUN_ACCESS_KEY_ID=your-access-key-id
ALIYUN_ACCESS_KEY_SECRET=your-access-key-secret
ALIYUN_REGION_ID=cn-hangzhou
ALIYUN_SECURITY_GROUP_ID=sg-xxxxxxxxx
ALIYUN_VSWITCH_ID=vsw-xxxxxxxxx
DDB_CONTAINER_IMAGE_URL=registry.cn-hangzhou.aliyuncs.com/your-repo/dolphindb:latest
CODE_SERVER_CONTAINER_IMAGE_URL=registry.cn-hangzhou.aliyuncs.com/your-repo/code-server:latest

# DolphinDB
DDB_HOST=localhost
DDB_PORT=8848
DDB_USER=admin
DDB_PASSWORD=123456

# OpenAI
OPENAI_API_KEY=your-openai-api-key
OPENAI_API_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL_NAME=gpt-3.5-turbo

# LLM
LLM_API_KEY=your-llm-api-key
LLM_BASE_URL=https://api.example.com
LLM_MODEL=google/gemini-2.5-flash

# RAG
RAG_MODE=local
GRAPH_RAG_API_URL=https://api.example.com/graph-rag
GRAPH_RAG_API_KEY=your-graph-rag-api-key
JINA_API_KEY=your-jina-api-key
FAISS_INDEX_PATH=my_docs_advanced.index
FAISS_CHUNKS_PATH=my_docs_chunks_advanced.pkl

# Email Configuration
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
EMAIL_FROM=your-email@gmail.com
EMAIL_VERIFICATION_EXPIRE_MINUTES=30